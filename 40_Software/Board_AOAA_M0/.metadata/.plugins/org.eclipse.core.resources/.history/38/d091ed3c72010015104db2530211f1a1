#include "can_handler.h"
#include "can.h"

void can_handler_init()
{
	CAN_Init(0x2301);			//set baud rate of 500kbps
	CAN_ConfigureMessages();	//configure all CAN message buffers
}

void can_handler_send()
{

}
void can_handler_receive()
{

}
void CAN_IRQHandler()
{

}

